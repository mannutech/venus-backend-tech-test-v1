{
  "name": "back-end-tech-test",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run -d src/database/migration-data-source.ts",
    "migration:run:prod": "node ./node_modules/typeorm/cli.js migration:run -d dist/src/database/migration-data-source.js",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert -d src/database/migration-data-source.ts",
    "migration:generate": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate -d src/database/migration-data-source.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.2.0",
    "pino": "^9.5.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^2.0.22",
    "pino-pretty": "^13.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
